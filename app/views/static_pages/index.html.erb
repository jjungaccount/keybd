
<div id="p-wrapper">
  <ul id="piano"> 
    <li><div onmousedown="PlayNote('C')" class="anchor"></div></li>
    <li><div onmousedown="PlayNote('D')" class="anchor"></div><span onmousedown="PlayNote('C#')"></span></li>
    <li><div onmousedown="PlayNote('E')" class="anchor"></div><span onmousedown="PlayNote('D#')"></span></li>
    <li><div onmousedown="PlayNote('F')" class="anchor"></div></li>
    <li><div onmousedown="PlayNote('G')" class="anchor"></div><span onmousedown="PlayNote('F#')"></span></li>
    <li><div onmousedown="PlayNote('A')" class="anchor"></div><span onmousedown="PlayNote('G#')"></span></li>
    <li><div onmousedown="PlayNote('B')" class="anchor"></div><span onmousedown="PlayNote('A#')"></span></li>
  </ul>
</div>

